[class^="{{prefix}}-"]:before {
    font-style: normal;
    font-weight: normal !important;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

{{# each codepoints }}
.{{ ../prefix }}-{{ @key }}:before {
    content: "\\{{ codepoint this }}";
}
{{/ each }}